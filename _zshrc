#!/bin/zsh
REPOS="$HOME/repos"

#History is important.
HISTFILE=~/.zsh_history
HISTSIZE=999999999
SAVEHIST=$HISTSIZE

#Source our Exports First
source $HOME/.exports

#Aliases
source $HOME/.aliases

#Functions
source $HOME/.functions

#Prezto
source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
prompt minimal

# added by travis gem
[ -f /Users/michael.ross/.travis/travis.sh ] && source /Users/michael.ross/.travis/travis.sh
