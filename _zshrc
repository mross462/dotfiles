#!/bin/zsh
#Source our Exports First
source $HOME/.exports

#Echo the tasklist
cat $HOME/repos/todo/todo

#Aliases
source $HOME/.aliases

#Functions
source $HOME/.functions

#ZSH Stuff
ZSH=$HOME/repos/dotfiles/_oh-my-zsh
ZSH_THEME="mross"
COMPLETION_WAITING_DOTS="true"

#Oh My ZSH Plugins
plugins=(aws chef git jira pip urltools vagrant)
source $ZSH/oh-my-zsh.sh
source /usr/local/bin/aws_zsh_completer.sh
export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting
