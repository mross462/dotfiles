#!/bin/zsh
#Source our Exports First
source $HOME/.exports

#ZSH Stuff
ZSH=$HOME/repos/dotfiles/_oh-my-zsh
ZSH_THEME="mross"
COMPLETION_WAITING_DOTS="true"

#Oh My ZSH Plugins
plugins=(chef git jira pip urltools vagrant)
source $ZSH/oh-my-zsh.sh

#Aliases
source $HOME/.aliases

#Functions
source $HOME/.functions

PATH=$PATH:$HOME/.rvm/bin # Add RVM to PATH for scripting
